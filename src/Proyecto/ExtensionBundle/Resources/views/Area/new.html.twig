{% extends "ProyectoExtensionBundle::layout.html.twig" %}

{% block title %}Areas{% endblock %}

{% block body %}
    <h1>Crear Area</h1>

    <form action="{{ path('area_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit">Crear</button>
        </p>
    </form>

    <ul class="record_actions">
        <li>
            <a href="{{ path('area') }}">
                Volver al listado
            </a>
        </li>
    </ul>
{% endblock %}
