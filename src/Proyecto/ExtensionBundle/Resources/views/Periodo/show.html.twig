<h1>Periodo</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Fechadesde</th>
            <td>{{ entity.fechaDesde|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Fechahasta</th>
            <td>{{ entity.fechaHasta|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('periodo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('periodo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('periodo_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
