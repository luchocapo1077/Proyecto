<h1>Proyecto list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Link</th>
            <th>Area</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
            <tr>
                <td><a href="{{ path('proyecto_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nombre }}</td>
                <td>{{ entity.link }}</td>
                <td>{{ entity.area }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('proyecto_show', { 'id': entity.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('proyecto_edit', { 'id': entity.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
    {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('proyecto_new') }}">
                Create a new entry
            </a>
        </li>
        <li><a href="{{ path('proyecto_map', {'name': 'Maps'}) }}">Maps</a></li>
    </ul>
